{
  "name": "git",
  "version": "1.0.0",
  "description": "Test Website for Kirby Jayes",
  "main": "index.html",
  "scripts": {
    "build": "npm run clean && npm run imagemin && npm run usemin",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run watch:all",
    "lite": "lite-server",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "usemin": "usemin music.html -d dist --htmlmin -o dist/music.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* -o dist/img"
  },


  "author": "Kirby Jayes",
  "license": "ISC",
  "devDependencies": {
    "imagemin-cli": "^5.1.0",
    "lite-server": "^2.6.1",
    "onchange": "^7.1.0",
    "parallelshell": "3.0.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },

  
  "dependencies": {
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
